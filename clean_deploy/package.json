{
  "name": "novaquote-financial-analyst",
  "version": "1.0.0",
  "description": "Expert ES Futures Trading Agent with AI & Real-time Data Analysis",
  "type": "module",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' run.ts --analyze",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "bot": "node --loader ts-node/esm --experimental-specifier-resolution=node src/discord_bot/working_bot.ts",
    "bot:full": "node --loader ts-node/esm --experimental-specifier-resolution=node src/discord_bot/index.ts",
    "bot:simple": "node src/discord_bot/bot_simple.cjs",
    "bot:commands": "node start_discord_bot.js",
    "bot:final": "node bot_final.cjs",
    "bot:test": "node test_bot_final.cjs",
    "bot:start": "node --loader ts-node/esm --experimental-specifier-resolution=node start_bot_simple.ts",
    "analyze": "node --loader ts-node/esm run.ts --analyze",
    "help": "node --loader ts-node/esm run.ts --help",
    "status": "node --loader ts-node/esm run.ts --status",
    "refresh": "node --loader ts-node/esm run.ts --refresh",
    "discord-chat": "node --loader ts-node/esm run.ts --discord-chat",
    "discord-profiles": "node --loader ts-node/esm run.ts --discord-profiles",
    "bot:nova": "node --loader ts-node/esm --experimental-specifier-resolution=node src/discord_bot/bot_with_nova.ts",
    "bot:nova": "node --loader ts-node/esm --experimental-specifier-resolution=node src/discord_bot/nova_bot_final.ts",
    "ingest:opml": "node --loader ts-node/esm --experimental-specifier-resolution=node src/backend/ingestion/OpmlIngestionScript.ts",
    "filter:news": "node --loader ts-node/esm --experimental-specifier-resolution=node src/backend/agents/NewsFilterAgent.ts",
    "filter:test": "node --loader ts-node/esm --experimental-specifier-resolution=node test_news_filter_dynamic.mjs",
    "scraper:test": "node --loader ts-node/esm --experimental-specifier-resolution=node src/x_scraper/index.ts",
    "bot:production": "NODE_ENV=production node src/discord_bot/production_bot.mjs",
    "bot:dev": "NODE_ENV=development node src/discord_bot/production_bot.mjs",
    "publish:realtime": "node cron_daily_x_scraper.mjs"
  },
  "keywords": [
    "trading",
    "es-futures",
    "ai",
    "financial-analysis",
    "discord-bot"
  ],
  "author": "Deamon",
  "license": "ISC",
  "dependencies": {
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sqlite3": "^5.1.7",
    "tsyringe": "^4.10.0",
    "ws": "^8.14.2",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
