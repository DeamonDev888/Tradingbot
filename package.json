{
  "name": "novaquote-financial-analyst",
  "version": "1.0.0",
  "description": "Expert ES Futures Trading Agent with AI & Real-time Data Analysis",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' run.ts --analyze",
    "test": "jest",
    "analyze": "ts-node run.ts --analyze",
    "continuous": "ts-node run.ts --continuous",
    "status": "ts-node run.ts --status",
    "help": "ts-node run.ts --help",
    "bot": "ts-node src/discord_bot/index.ts",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "refresh": "ts-node run.ts --refresh",
    "refresh:force": "ts-node run.ts --refresh --force",
    "vix:scrape": "ts-node run.ts --vix-scrape",
    "vix:analyze": "ts-node run.ts --vix-analyze",
    "scrape:mw": "ts-node run.ts --scrape-mw",
    "scrape:inv": "ts-node run.ts --scrape-inv",
    "db:init": "ts-node create_database.ts",
    "db:stats": "ts-node run.ts --status",
    "pipeline": "ts-node src/backend/scripts/run_news_data_pipeline.ts",
    "sentiment": "ts-node src/backend/scripts/run_agent_sentiment.ts",
    "vortex500": "ts-node -e \"import { Vortex500Agent } from './src/backend/agents/Vortex500Agent'; const agent = new Vortex500Agent(); agent.analyzeMarketSentiment(true).then(console.log);\"",
    "vortex500:force": "ts-node -e \"import { Vortex500Agent } from './src/backend/agents/Vortex500Agent'; const agent = new Vortex500Agent(); agent.analyzeMarketSentiment(true).then(console.log);\"",
    "vix:legacy": "ts-node src/backend/scripts/vix_simple_example.ts",
    "vix:live": "ts-node src/backend/scripts/get_vix_data.ts",
    "vix:dtc": "ts-node src/backend/scripts/vix_dtc_binary.ts",
    "vix:optimized": "ts-node src/backend/scripts/vix_dtc_optimized.ts",
    "vix:persistent": "ts-node src/backend/scripts/vix_persistent.ts",
    "vix:debug": "ts-node src/backend/scripts/vix_debug.ts",
    "vix:exchanges": "ts-node src/backend/scripts/vix_exchanges.ts",
    "vix:discovery": "ts-node src/backend/scripts/vix_discovery.ts",
    "vix:file": "ts-node src/backend/scripts/vix_sierra_file_reader.ts",
    "crypto:file": "ts-node src/backend/scripts/crypto_sierra_file_reader.ts",
    "vix:setup": "ts-node src/backend/scripts/setup_vix.ts",
    "vix:multi": "ts-node src/backend/scripts/vix_multi_source.ts",
    "vix:database": "ts-node src/backend/scripts/vix_database_integration.ts",
    "market:unified": "ts-node src/backend/scripts/unified_market_data.ts",
    "db:analyze": "ts-node src/backend/scripts/database_analysis.ts",
    "db:quick": "ts-node src/backend/scripts/quick_db_analysis.ts",
    "db:cleanup": "ts-node src/backend/scripts/db_cleanup.ts",
    "test:scrapers": "ts-node src/backend/scripts/test_scrapers.ts",
    "test:playwright": "ts-node src/backend/scripts/test_playwright_vix.ts",
    "scrape:te": "ts-node src/backend/scripts/scrape_trading_economics.ts",
    "rouge:pulse": "ts-node src/backend/scripts/run_rouge_pulse.ts"
  },
  "keywords": [
    "trading",
    "es-futures",
    "ai",
    "financial-analysis",
    "scraping"
  ],
  "author": "Deamon",
  "license": "ISC",
  "dependencies": {
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.25.1",
    "dotenv": "^16.3.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "playwright": "^1.56.1",
    "sqlite3": "^5.1.7",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
